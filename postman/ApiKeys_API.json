            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/api-keys/{{apiKeyId}}",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "api",
                "v1",
                "api-keys",
                "{{apiKeyId}}"
              ]
            },
            "description": "Met à jour une clé API existante"
          },
          "response": []
        },
